{
  "extends": "airbnb/legacy",
  "env": {
    "jasmine": true,
    "node": true
  },
  "globals": {
    "module": true,
    "inject": true,
    "browser": false,
    "element": false,
    "by": false,
    "$": false,
    "$$": false
  },
  "rules": {
    "indent": [2, 4, {"SwitchCase": 1}],
    "quotes": [2, "double", "avoid-escape"],
    "comma-dangle": [2, "never"],
    "strict": [2, "global"],
    "padded-blocks": [2, "always"],
    "func-names": 0,
    "id-length": 0,
    "space-before-function-paren": [2, "never"]
  }
}
